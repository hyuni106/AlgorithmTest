<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <layer-list>
            <item
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp">
                <shape android:shape="rectangle">
                    <corners android:radius="10dp" />
                    <solid android:color="#9bdcd7"></solid>
                </shape>
            </item>
        </layer-list>
    </item>

    <item android:state_pressed="true">
        <layer-list>
            <item
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp">
                <shape android:shape="rectangle">
                    <corners android:radius="10dp" />
                    <solid android:color="#92c1be"></solid>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>