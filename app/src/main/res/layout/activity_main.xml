<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:context="com.example.user.algorithmtest.MainActivity">

    <Button
        android:id="@+id/test01Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="3의 배수와 5의 배수의 총합 구하기"/>

    <Button
        android:id="@+id/test02Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="별찍기"/>

    <Button
        android:id="@+id/test03Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="구구단 2~9단 출력"/>

    <Button
        android:id="@+id/test04Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ㅇㅅㅇ"/>

    <Button
        android:id="@+id/test05Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="채팅형 숫자 야구게임"/>

    <Button
        android:id="@+id/test06Btn"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="로또'ㅅ'"/>

</LinearLayout>
